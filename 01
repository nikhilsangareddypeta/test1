collectionOut = new DataTable();

try
{	
	// Create DataColumns
	
	DataTable dt = collectionIn.Clone();
	
	int i = dt.Columns[fieldName].Ordinal;
	
	dt.Columns.Remove(fieldName);
	
	dt.Columns.Add(fieldName, typeof(Decimal)).SetOrdinal(i);

	// Fill DataRows
	foreach	(DataRow row in collectionIn.Rows)
	{
		dt.ImportRow(row);
	}
	
	// Output Table
	collectionOut = dt;
}
catch (Exception e)
{
	Console.WriteLine("An error occurred: " + e);
}


