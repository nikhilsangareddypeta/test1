//DataTable New_Collection = new DataTable();
colOut = new DataTable();

//Add Columns by looping Name column
foreach (DataRow row in colIn.Rows)
{
	if (!colOut.Columns.Contains(row[0].ToString()))
	{
		colOut.Columns.Add(row[0].ToString());
	}
}

int newRowCount = 0;
int rowCount = 0;
int colCount = 0;
foreach (var row in colIn.Rows)
{
	if (colCount == 0)
	{
		colOut.Rows.Add();
	}
	if (colCount+1 == colOut.Columns.Count)
	{
		colOut.Rows[newRowCount][colCount] = colIn.Rows[rowCount][1].ToString();
		colCount = 0;
		newRowCount++;
	}
	else
	{
		colOut.Rows[newRowCount][colCount] = colIn.Rows[rowCount][1].ToString();
		colCount++;
	}
	rowCount++;
