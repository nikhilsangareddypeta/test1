{
	DateTime newDt = new DateTime();
	DateTime.TryParse(inColl.Rows[i][fieldName].ToString(), out newDt);
	inColl.Rows[i]["DateTimeCol"] = newDt.ToLocalTime();
 
}


// sort on the new column
