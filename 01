e" /></inputs><onsuccess>c2cc0e86-6449-47ca-ad5e-38a510d4865f</onsuccess><processid>733c2840-8162-465a-a8f2-7d023087c369</processid></stage><stage stageid="c2cc0e86-6449-47ca-ad5e-38a510d4865f" name="WriteColl Fast" type="Code"><subsheetid>65011082-99fa-4fd0-ad23-c2c819f9beb7</subsheetid><loginhibit /><display x="45" y="-15" w="120" h="30" /><font family="Tahoma" size="10" style="Regular" color="000000" /><inputs><input type="number" name="handle" expr="[handle]" /><input type="text" name="workbookname" expr="[workbookname]" /><input type="text" name="worksheetname" expr="[worksheetname]" /><input type="text" name="cellref" expr="[cellref]" /><input type="collection" name="collection" expr="[collection]" /><input type="flag" name="includecolnames" expr="[includecolnames]" /></inputs><outputs><output type="text" name="Message" stage="Message" /><output type="flag" name="Success" stage="Success" /></outputs><onsuccess>3c77f92b-b61c-4062-be7d-a25d89ef18e7</onsuccess><code><![CDATA[Dim ws, cref as Object
Dim dt As New Datatable()
Dim c, r, colNum, rowNum as Integer


Try
ws = GetWorksheet(handle,workbookname,worksheetname)
cref = ws.Range(cellref)
rowNum = cref.row
colNum = cref.column
dt = collection
dim arr(dt.Rows.Count, dt.Columns.Count) as Object

For r = 0 to dt.Rows.Count-1
For c = 0 to dt.Columns.Count-1
arr(r, c)= dt.Rows(r).Item(c)
Next
Next

'write column headers
If includecolnames Then
c = 0
For Each col as DataColumn in dt.Columns
Try
ws.Cells(rowNum,colNum+c).Value = col.ColumnName
Catch ex as Exception
Exit For
End Try
c += 1
Next
rowNum += 1
End If

'write rows
ws.Range(ws.Cells(rowNum,colNum), ws.Cells(dt.Rows.Count+rowNum, dt.Columns.Count)).Value = arr
       Success = True
Catch e as Exception
       Success = False
Message = e.Message
Finally
ws = Nothing
cref = Nothing
dt.Clear()
End Try]]></code></stage><stage stageid="6a4faa0c-35ab-47c8-87a8-66b881385a86" name="cellref" type="Data"><subsheetid>65011082-99fa-4fd0-ad23-c2c819f9beb7</subsheetid><display x="-135" y="-30" w="120" h="30" /><font family="Tahoma" size="10" style="Regular" color="000000" /><datatype>text</datatype><initialvalue /><private /><alwaysinit /></stage><stage stageid="d3114527-b28e-4d12-bcb2-9e928cdeb8b0" name="Message" type="Data"><subsheetid>65011082-99fa-4fd0-ad23-c2c819f9beb7</subsheetid><display x="-270" y="105" w="120" h="30" /><datatype>text</datatype><initialvalue /><private /><alwaysinit /></stage><stage stageid="955d6b03-dcc5-40e2-ad4b-b73f18a17833" name="Success" type="Data"><subsheetid>65011082-99fa-4fd0-ad23-c2c819f9beb7</subsheetid><display x="-150" y="105" w="120" h="30" /><datatype>flag</datatype><initialvalue /><private /><alwaysinit /></stage></process></object>
