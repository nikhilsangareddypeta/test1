if(!tblLookup.Columns.Contains(strRefCol))
{
	throw new System.Exception(String.Format("Collection does not contain reference column {0}",strRefCol));	
}

// Throw an exception if input collection doesn't contain the column holding the value to be returned
else if (!tblLookup.Columns.Contains(strOutCol))
{
	throw new System.Exception(String.Format("Collection does not contain return column {0}",strOutCol));
}

// Otherwise create a dataview and loop through the rows returned, updating values as appropriate	
else
{
	DataView dv = new DataView(tblLookup);
	dv.RowFilter = "[" + strRefCol + "]" + " = '" + strSearchVal + "'";
	
	// Throw an exception if no rows were returned by the filter (lookup value not found)
	if(dv.Count == 0)
	{
		throw new System.Exception("Lookup value '" + strSearchVal + "' not found in reference column '" + strRefCol + "'");
	}
	
	// If updateAll is true, loop through all the rows (1 or more) returned by the row filter 
	if(updateAll)
	{
		foreach(DataRowView rowView in dv)
		{
			DataRow row = rowView.Row;
			row[strOutCol] = strNewVal;
		}	
	}
	// If updateAll is false, just update the first row returned by the filter
	else
	{
		DataRow row = dv[0].Row;
		row[strOutCol] = strNewVal;
	}
}

output = tblLookup;]]></code></stage><stage stageid="cdcb712c-2eda-4ae9-8c2d-8db57b725fbf" name="lookup table" type="Collection"><subsheetid>44afbd82-19ea-48c4-9939-e9add8b371f9</subsheetid><loginhibit onsuccess="true" /><display x="-120" y="-90" w="90" h="30" /><datatype>collection</datatype><private /><alwaysinit /></stage><stage stageid="67744d72-2a12-4235-899b-51553606bd45" name="lookup value" type="Data"><subsheetid>44afbd82-19ea-48c4-9939-e9add8b371f9</subsheetid><loginhibit onsuccess="true" /><display x="-120" y="0" w="90" h="30" /><datatype>text</datatype><initialvalue /><private /><alwaysinit /></stage><stage stageid="95d7b3ae-1ba1-43b4-9775-c03d7e61fa42" name="reference column" type="Data"><subsheetid>44afbd82-19ea-48c4-9939-e9add8b371f9</subsheetid><loginhibit onsuccess="true" /><display x="-120" y="-60" w="90" h="30" /><datatype>text</datatype><initialvalue /><private /><alwaysinit /></stage><stage stageid="23822a94-cf75-4b02-864d-7c625e03211b" name="Append Field (Flag)" type="SubSheetInfo"><subsheetid>f3252208-c726-42d9-839d-52520d9bb789</subsheetid><loginhibit onsuccess="true" /><narrative>Appends a field to a collection and allows you to provide a default value as flag for the field.</narrative><display x="-195" y="-105" w="150" h="90" /><font family="Tahoma" size="10" style="Regular" color="000000" /></stage><stage stageid="bfcd2fc6-6509-473f-96ec-07f5d983300d" name="Field Name" type="Data"><subsheetid>f3252208-c726-42d9-839d-52520d9bb789</subsheetid><loginhibit onsuccess="true" /><display x="-195" y="30" w="150" h="30" /><font family="Tahoma" size="10" style="Regular" color="000000" /><datatype>text</datatype><initialvalue /><private /><alwaysinit /></stage><stage stageid="1eb594cc-8226-4cb7-b898-51d412d06347" name="Append Flag Field" type="Code"><subsheetid>f3252208-c726-42d9-839d-52520d9bb789</subsheetid><loginhibit onsuccess="true" /><display x="15" y="-15" /><font family="Tahoma" size="10" style="Regular" color="000000" /><inputs><input type="co